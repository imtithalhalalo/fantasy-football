datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  team     Team?
}

model Team {
  id      Int      @id @default(autoincrement())
  name    String
  budget  Int
  userId  Int      @unique
  user    User     @relation(fields: [userId], references: [id])
}
